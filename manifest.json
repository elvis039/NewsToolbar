{
  "manifest_version": 2,
  "name": "NewsToolbar",
  "description": "CalHacks5.0 - ",
  "version": "0.1",
  "browser_action": {
    "default_icon": {
      "19": "icons/icon.png"
    },
    "default_popup": "view/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.3.1.min.js", "js/content.js"]
    }
  ],
  "background": {
    "scripts": ["jquery-3.3.1.min.js", "js/background.js"]
  }
}
